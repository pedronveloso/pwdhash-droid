{
"problemMatcher": [
{
"owner": "android-lint line",
"pattern": [
{
"regexp": "^\\s*?(.+):(\\d+):\\s+(Error|Warning):\\s+(.+)\\[(.+)\\]\\s*$",
"file": 1,
"line": 2,
"severity": 3,
"message": 4,
"code": 5
}
]
},
{
"owner": "android-lint file",
"pattern": [
{
"regexp": "^\\s*?(.+):\\s+(Error|Warning):\\s+(.+)\\[(.+)\\]\\s*$",
"file": 1,
"severity": 2,
"message": 3,
"code": 4,
"kind": "file"
}
]
}
]
}
